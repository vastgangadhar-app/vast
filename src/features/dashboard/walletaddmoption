            setPhonepeMax(phonepeMaxValue);

            const phonepeam = parseFloat(amount);
            const phonepeMin1 = parseFloat(phonepeMinValue);
            const phonepemaxx1 = parseFloat(phonepeMaxValue);

            if (phonepeam >= phonepeMin1 && phonepeam <= phonepemaxx1) {
