            const url = data["RESULT"];
            const supported = await Linking.canOpenURL(url);

            if (Status) {
                if (supported) {
                    await Linking.openURL(url);
                } else {
                    console.log("Don't know how to open URI: ", url);
                }
            } else {
                showAlert(url);
            }

            setIsLoading(false)

        } catch (error) {
            setIsLoading(false)

